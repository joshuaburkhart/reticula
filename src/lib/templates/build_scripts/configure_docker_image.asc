#!/bin/bash
cp ${dockerRelPath} ./Dockerfile
cp ${p3scriptPath} ./executable.py
docker login
docker build_scripts -t ${dockerAcctNme}/${dockerImgNme} .
docker save ${dockerAcctNme}/${dockerImgNme} | gzip > ./saved${dockerImgNme}.tar.gz
docker push ${dockerAcctNme}/${dockerImgNme}
scp ./saved${dockerImgNme}.tar.gz ${exaclAcctNme}@exahead1.ohsu.edu:${lusImgPath}
rm ./Dockerfile
rm ./executable.py
rm ./saved${dockerImgNme}.tar.gz
